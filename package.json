{
  "name": "react-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "^0.18.0",
    "cross-fetch": "^3.0.3",
    "md5": "^2.2.1",
    "npm-run-all": "^4.1.5",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-health-card": "^1.3.4",
    "react-redux": "^7.0.3",
    "react-router-dom": "^5.0.0",
    "react-scripts": "3.0.1",
    "redux": "^4.0.1",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "simple-cache-provider": "^0.10.0",
    "styled-components": "^4.2.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "npm run build:css && react-scripts build",
    "build:css": "cross-env NODE_ENV=production postcss src/assets/styles/index.src.css -o src/assets/styles/index.css",
    "watch:css": "postcss src/assets/styles/index.src.css -o src/assets/styles/index.css -w",
    "dev": "concurrently --kill-others \"npm run watch:css\" \"npm start\""
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "^1.2.0",
    "autoprefixer": "^9.6.0",
    "concurrently": "^4.1.1",
    "cross-env": "^5.2.0",
    "postcss-cli": "^6.1.2",
    "postcss-import": "^12.0.1",
    "postcss-preset-env": "^6.6.0",
    "redux-devtools": "^3.5.0",
    "redux-devtools-extension": "^2.13.8",
    "tailwindcss": "^1.0.4"
  }
}
